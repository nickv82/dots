"*":
  core:
    ignoredNames: [
      "*.pyc"
      "**/dist"
      "**/bower_components"
      "**/node_modules"
      "**/.idea"
      "**/.tmp"
      "**/.sass-cache"
      "**/*.iml"
    ]
    disabledPackages: [
      "git-diff"
    ]
    themes: [
      "seti-ui"
      "base16-tomorrow-dark-theme"
    ]
  "atom-beautify":
    _analyticsUserId: "4db92f04-a106-426a-9bf8-f045b136c3f4"
    beautifyOnSave: true
  "exception-reporting":
    userId: "58b76047-44a3-519b-fbde-17dee35892ff"
  welcome:
    showOnStartup: false
  metrics:
    userId: "55cf1a036d5e07e135badaf2309d4b0e35cf9c3b"
  editor:
    fontFamily: "Ubuntu Mono"
    fontSize: 15
    preferredLineLength: 100
    invisibles: {}
  "tree-view":
    hideIgnoredNames: true
  linter: {}
  react:
    jsxTagStartPattern: "(?x)((^|=|return)\\s*<([^!/?](?!.+?(</.+?>))))"
    jsxComplexAttributePattern: "(?x)\\{ [^}\"']* $|\\( [^)\"']* $"
    decreaseIndentForNextLinePattern: "(?x) />\\s*(,|;)?\\s*$ | ^\\s*\\S+.*</[-_\\.A-Za-z0-9]+>$"
